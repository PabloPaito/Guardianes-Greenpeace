                                    <Image x:Name="CountryFlag" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="UniformToFill" Width="26" Height="26"  
                                           Source="../../Resources/Images/Flags/1-35.png" MouseLeftButtonDown="changePlayingCountryButton_click"  Cursor="Hand"/>
                                    <TextBlock x:Name="RegionNametxt" Text="-" Foreground="#fff" HorizontalAlignment="Center" FontSize="19" Margin="5,0,0,0" />
                                </StackPanel>
                                <Grid  x:Name="InformationMenu" Margin="40,0,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="30" />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                        <RowDefinition />
                                    </Grid.RowDefinitions>

                                    <!--REGION POINTS-->
                                    <Border Grid.Row="0" Grid.ColumnSpan="2" Height="3" Width="160" Margin="0,3" BorderBrush="#FF9cb713" Background="#FF798822" BorderThickness="0,1,0,1" />
                                    <Image x:Name="RegionalPts" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="UniformToFill"  Width="25" Height="25"  
                                          Margin="0,2,0,2" Source="../../Resources/Images/LeftMenuIcons/regionalpts.png" ToolTipService.ToolTip="{Binding Path=RegionalScore, Source={StaticResource Labels}}"/>
                                    <StackPanel Grid.Column="1" Orientation="Horizontal" Grid.Row="1" ToolTipService.ToolTip="{Binding Path=RegionalScore, Source={StaticResource Labels}}">
                                        <TextBlock x:Name="RegionScore" Text="-"  Visibility="Visible" FontFamily="/Earthwatchers.UI;component/Resources/MyriadPro-Regular.otf#Myriad Pro" VerticalAlignment="Center" Margin="10,0,0,0" FontSize="15"/>
                                        <TextBlock x:Name="RegionScorePts" Text="{Binding Path=NavBar9, Source={StaticResource Labels}}"  FontFamily="/Earthwatchers.UI;component/Resources/MyriadPro-Regular.otf#Myriad Pro" VerticalAlignment="Center" Margin="5,0,0,0" FontSize="10" FontWeight="Bold" />
                                    </StackPanel>

                                    <!--PLOTS VERIFIED-->
                                    <Border Grid.Row="2" Grid.ColumnSpan="2" Height="3" Width="160" Margin="0,3" BorderBrush="#FF9cb713" Background="#FF798822" BorderThickness="0,1,0,1" />
                                    <Image x:Name="Plots" Grid.Row="3" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Uniform" Width="25" Height="25"  
                                          Margin="0,2,0,2" Source="../../Resources/Images/LeftMenuIcons/plots.png" ToolTipService.ToolTip="{Binding Path=PlotsVerified, Source={StaticResource Labels}}"/>
                                    <TextBlock x:Name="TotalVerifications" Grid.Row="3" Grid.Column="1" Visibility="Visible" Text="-"  Margin="10,0,0,0" VerticalAlignment="Center" Foreground="White"
                                               ToolTipService.ToolTip="{Binding Path=PlotsVerified, Source={StaticResource Labels}}"/>

                                    <!--PRECISION PERCENTAGE-->
                                    <Border Grid.Row="4" Grid.ColumnSpan="2" Height="3" Width="160" Margin="0,3" BorderBrush="#FF9cb713" Background="#FF798822" BorderThickness="0,1,0,1" />
                                    <Image x:Name="PrecisionImg" Grid.Row="5" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="Uniform" Width="25" Height="25" 
                                          Margin="0,2,0,2" Source="/Resources/Images/LeftMenuIcons/verifiedplotsw.png" ToolTipService.ToolTip="{Binding Path=PrecisionPercentage, Source={StaticResource Labels}}"/>
                                    <TextBlock x:Name="PrecisionTxt" Grid.Row="5" VerticalAlignment="Center" Grid.Column="1" Text="- %" Margin="10,0,0,0" Foreground="#fff" 
                                               FontFamily="/Earthwatchers.UI;component/Resources/MyriadPro-Bold.otf#Myriad Pro" ToolTipService.ToolTip="{Binding Path=PrecisionPercentage, Source={StaticResource Labels}}"/>

                                    <Border Grid.Row="6" Grid.ColumnSpan="2" Height="3" Width="160" Margin="0,3" BorderBrush="#FF9cb713" Background="#FF798822" BorderThickness="0,1,0,1" />

                                </Grid>
                            </StackPanel>
                            <!--TERCERA COLUMNA FIN-->

                            <!--CUARTA COLUMNA COMMON BUTTONS-->
                            <StackPanel Orientation="Horizontal" Grid.Row="5" Margin="6,0">
                                <!--CHANGE COUNTRY-->
                                <StackPanel Width="60" VerticalAlignment="Center" MouseEnter="ChangeCountryHooverOn" MouseLeave="ChangeCountryHooverOff" MouseLeftButtonDown="changePlayingCountryButton_click"  Cursor="Hand" ToolTipService.ToolTip="{Binding Path=GoToAnotherRegion, Source={StaticResource Labels}}">
                                    <Image x:Name="ChangeRegion" HorizontalAlignment="Center" Source="Resources/Images/LeftMenuIcons/changeregion.png" Stretch="Uniform" Width="40" Opacity="1"/>
                                    <TextBlock  Margin="0,2,0,-3" HorizontalAlignment="Center" Text="{Binding Path=Change, Source={StaticResource Labels}}" VerticalAlignment="Center" FontSize="9" />
                                    <TextBlock  HorizontalAlignment="Center" Text="{Binding Path=Region, Source={StaticResource Labels}}" VerticalAlignment="Center" FontSize="9" />
                                </StackPanel>
                                <!--NEW PLOT-->
                                <StackPanel x:Name="newPlotDiv" Width="60" VerticalAlignment="Center" MouseEnter="NewPlotHooverOn" MouseLeave="NewPlotHooverOff" MouseLeftButtonDown="reshuffleButton_click" Cursor="Hand" ToolTipService.ToolTip="{Binding Path=NavBar14, Source={StaticResource Labels}}">
                                    <Image x:Name="NewPlot" HorizontalAlignment="Center" Source="Resources/Images/LeftMenuIcons/newplot.png" Stretch="Uniform" Width="40"  Opacity="1"/>
                                    <TextBlock Margin="0,2,0,-3" HorizontalAlignment="Center" TextWrapping="Wrap" Text="{Binding Path=New, Source={StaticResource Labels}}" VerticalAlignment="Center" FontSize="9" />
                                    <TextBlock HorizontalAlignment="Center" Text="{Binding Path=Plot, Source={StaticResource Labels}}" VerticalAlignment="Center" FontSize="9" />
                                </StackPanel>
                                <!--MY PLOT-->
                                <StackPanel x:Name="myPlotDiv" Width="60" VerticalAlignment="Center" MouseEnter="MyPlotHooverOn" MouseLeave="MyPlotHooverOff" MouseLeftButtonDown="myPlotButton_click" Cursor="Hand" ToolTipService.ToolTip="{Binding Path=NavBar12, Source={StaticResource Labels}}" >
                                    <Image x:Name="MyPlot" HorizontalAlignment="Center" Source="Resources/Images/LeftMenuIcons/myplot.png" Stretch="Uniform" Width="40" Opacity="1"/>
                                    <TextBlock x:Name="btnMyLand" Margin="0,2,0,-3" HorizontalAlignment="Center" Text="{Binding Path=My, Source={StaticResource Labels}}" VerticalAlignment="Center" FontSize="9" />
                                    <TextBlock HorizontalAlignment="Center" Text="{Binding Path=Plot, Source={StaticResource Labels}}" VerticalAlignment="Center" FontSize="9" />
                                </StackPanel>
                            </StackPanel>
                            <!--Borde de "Mi Parcela" titilando-->
                            <Border x:Name="TutorialMyPlotButton" Grid.Row="5" Width="60" Height="80" MouseLeftButtonDown="TutorialMyPlotButtonClick" Visibility="Collapsed" Cursor="Hand" 
                                            Background="Transparent" BorderThickness="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,8,6" Canvas.ZIndex="3">
                                <Rectangle x:Name="btnMyLandHighlight" Opacity="0" Grid.Row="3" Grid.Column="2" Width="60" Height="80" Fill="Transparent" MouseLeftButtonDown="TutorialMyPlotButtonClick" Cursor="Hand" Stroke="#FFb6d220" StrokeThickness="3" />
                            </Border>
                            <Border Grid.Row="6" Height="3" BorderBrush="#FF9cb713" Background="#FF3c4705" BorderThickness="0,1,0,1" />
                            <!--CUARTA COLUMNA FIN-->

                            <!--QUINTA COLUMNA ZOOM-->
                            <Border MouseEnter="ZoomOutHooverOn" MouseLeave="ZoomOutHooverOff" MouseLeftButtonDown="zoomOutButton_click" Cursor="Hand" Width="97"  HorizontalAlignment="Left"
                                    Grid.Row="7" ToolTipService.ToolTip="{Binding Path=NavBar4, Source={StaticResource Labels}}" >
                                <Grid VerticalAlignment="Center">
                                    <Image x:Name="ZoomOut" HorizontalAlignment="Center" Height="40" Width="40" Source="Resources/Images/LeftMenuIcons/zoomout.png" Stretch="Uniform" />
                                </Grid>
                            </Border>
                            <Border MouseEnter="ZoomInHooverOn" MouseLeave="ZoomInHooverOff" MouseLeftButtonDown="zoomInButton_click" Cursor="Hand" Width="97"  HorizontalAlignment="Right" 
                                    Grid.Row="7" ToolTipService.ToolTip="{Binding Path=NavBar3, Source={StaticResource Labels}}">
                                <Grid VerticalAlignment="Center" >
                                    <Image x:Name="ZoomIn" HorizontalAlignment="Center"  Height="40" Width="40" Source="Resources/Images/LeftMenuIcons/zoomin.png" Stretch="Uniform" />
                                </Grid>
                            </Border>
                            <Border Grid.Row="8" Height="3" BorderBrush="#FF9cb713" Background="#FF3c4705" BorderThickness="0,1,0,1" />
                            <!--QUINTA COLUMNA FIN-->                           
                          </Grid>
                    </Border>
                    <!--MENU IZQUIERDO FIN-->
                </Grid>
            </Grid>

            <!--Texto "Puede tener nubes"-->
            <TextBlock x:Name="NotificationText" Visibility="Collapsed" IsHitTestVisible="False" TextWrapping="Wrap" Opacity="0" FontSize="50" Foreground="White" HorizontalAlignment="Center" Margin="210,0,10,0" TextAlignment="Center" VerticalAlignment="Center" FontFamily="/Earthwatchers.UI;component/Resources/MyriadPro-Regular.otf#Myriad Pro">
                <TextBlock.Effect>
                    <DropShadowEffect BlurRadius="10" ShadowDepth="0" />
                </TextBlock.Effect>
            </TextBlock>
            <!--Recuadro para buscar los GeoHexCode (ADMIN)-->
            <TextBox x:Name="TestGeoHexTextBox" Width="100" VerticalAlignment="Bottom" HorizontalAlignment="Left" Margin="220,0,0,10" Visibility="Collapsed" KeyUp="TestGeoHexTextBox_KeyUp" />


            <!--Loading-->
            <Grid x:Name="loadinAnim" Visibility="Collapsed" Canvas.ZIndex="9">
                <loaders:_3D Margin="0,0,0,0" />
                <TextBlock x:Name="loadingAnimText" Text="{Binding Path=Cargando, Source={StaticResource Labels}}" TextWrapping="Wrap" MaxWidth="200" TextAlignment="Center" Margin="0,60,0,0" FontFamily="Arial" FontSize="13" Foreground="#FF333333" VerticalAlignment="Center" HorizontalAlignment="Center" />
            </Grid>
        </Grid>

        <MediaElement x:Name="pointsSound" Width="1" Height="1" VerticalAlignment="Top" HorizontalAlignment="Left" AutoPlay="False" MediaFailed="pointsSound_MediaFailed" Source="Resources/02.mp3" />

    </Grid>
</UserControl>
